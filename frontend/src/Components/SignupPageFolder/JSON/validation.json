    {
        "name": "email",
        "conditions": [
            {
                "condition": "obj.email === 'undefined' || obj.email === ''",
                "errmessage": "email is require"
            },
            {
                "condition": "!obj.email.includes('@outlook') && !obj.email.includes('@gmail')",
                "errmessage": "please enter valid email"
            }
        ]
    },
    {
        "name": "mobile",
        "conditions": [
            {
                "condition": "obj.mobile === 'undefined' || obj.mobile === ''",
                "errmessage": "mobile is require"
            },
            {
                "condition": "!/[0-9]/.test(obj.mobile)",
                "errmessage": "Mobile only contain number"
            },
            {
                "condition": "obj.mobile.length !== 10",
                "errmessage": "Mobile only contain 10 digits"
            }
        ]
